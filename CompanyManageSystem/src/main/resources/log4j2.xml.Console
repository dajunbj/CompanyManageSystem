<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="MyApp" packages="">
  <Appenders>
  <!-- コンソール出力 -->
    <Console name="STDOUT" target="SYSTEM_OUT">
      <PatternLayout pattern="%m%n"/>
    </Console>
  </Appenders>
  <Loggers>
  <!-- level = all,trace,info,error,debug -->
    <Root level="all">
      <AppenderRef ref="STDOUT"/>
    </Root>
  </Loggers>
</Configuration>